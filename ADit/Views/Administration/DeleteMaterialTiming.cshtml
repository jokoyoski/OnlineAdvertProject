@model ADit.Interfaces.IMaterialTypeTimingView




@section PageTitle{
    Delete Material Type Timing
}

<div class="jumbotron">
    <div class="columns" style="font-size: 12px; margin-left: 15px; margin-top: 15px; width: 100%;">
        <div class="validation-summary-errors">
            @Html.ValidationSummary()
            @Model.ProcessingMessage
        </div>
    </div>
    @using (Html.BeginForm("DeleteMaterialTiming", "Administration", FormMethod.Post))
    {
        @Html.AntiForgeryToken()

        <input type="hidden" id="materialTimingId" name="materialTimingId" value="@Model.MaterialTypeTimingId" />






        <div class="text-danger text-center">Are you sure you want to delete this Material Timing?</div>
        <p class="text-center text-bold" style="font-weight: bold">Material Timing</p>
        <p class="text-center text-bold">@Model.ServiceTypeCode</p>





        <div align="center">

            <input class="btn btn-primary" id="btnSubmit" name="submitButton" style="width: 80px;" type="submit" value="Delete" />
            <a href="/Administration/MaterialTiming" class="btn btn-danger" id="btnCancel" name="submitButton" style="width: 80px;color:white">Cancel</a>

        </div>
    }
</div>


